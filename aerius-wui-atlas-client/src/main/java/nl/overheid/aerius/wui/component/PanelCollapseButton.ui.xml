<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:e="urn:import:nl.overheid.aerius.wui.main.ui.editor"
  xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:w="urn:import:nl.overheid.aerius.wui.widget"
  xmlns:c="urn:import:nl.overheid.aerius.wui.component">
  <ui:with field='i18n' type='nl.overheid.aerius.wui.i18n.ApplicationMessages' />
  <ui:with field='img' type='nl.overheid.aerius.wui.resources.ImageResources' />
  <ui:with field='res' type='nl.overheid.aerius.wui.resources.R.ApplicationResource' />
  <ui:style>
    .button {
    height: 50px;
    width: 50px;
    }
  </ui:style>
  <g:SimplePanel>
    <c:PanelVisibilityButton ui:field="visibilityButton" addStyleNames="{style.button}" />
  </g:SimplePanel>
</ui:UiBinder>
